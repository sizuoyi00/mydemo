<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="demo"
             targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressAllComments"
                      value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/sy_demo?characterEncoding=utf-8&amp;useSSL=true"
                        userId="root"
                        password="Jifen004"/>

        <javaModelGenerator targetPackage="cn.sy.demo.model"
                            targetProject="src/main/java">
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="dao.mapper.demo"
                         targetProject="src/main/resources">
        </sqlMapGenerator>

        <javaClientGenerator targetPackage="cn.sy.demo.dao.demo"
                             targetProject="src/main/java"
                             type="XMLMAPPER">
        </javaClientGenerator>

        <table schema="sy_demo"
            tableName="jwtUserDo"
            domainObjectName="User">
        <ignoreColumn column="modify_time"/>
        <ignoreColumn column="create_time"/>
        </table>

    </context>
</generatorConfiguration>
